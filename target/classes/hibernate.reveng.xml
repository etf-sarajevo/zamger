<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
	</type-mapping>
	<table-filter match-name="acl" match-catalog="zamger"></table-filter>
	<table-filter match-name="acl_tip" match-catalog="zamger"></table-filter>
	<table-filter match-name="acl_url" match-catalog="zamger"></table-filter>
	<table-filter match-schema="zamger" match-name=".*"
		exclude="true">
	</table-filter>



</hibernate-reverse-engineering>