	<subDataset name="Spisak predmeta" uuid="4f84f3c3-9c8a-4065-98df-825b59731f7e">
		<parameter name="zahtjev" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT pp.sifra, pp.naziv naziv_predmeta, pp.ects, ko.ocjena, 
    ko.datum_u_indeksu, pk.semestar semestar_ocjene, 
    ts.ciklus ciklus_ocjene
FROM konacna_ocjena as ko, ponudakursa as pk, 
	pasos_predmeta as pp, student_predmet as sp, studij as s, 
	tipstudija as ts, zahtjev_za_potvrdu zzp
WHERE ko.student=zzp.student and ko.pasos_predmeta=pp.id 
	and ko.predmet=pk.predmet 
	and ko.akademska_godina=pk.akademska_godina 
	and pk.id=sp.predmet and sp.student=zzp.student 
	and pk.studij=s.id and s.tipstudija=ts.id 
	and ko.ocjena>5 and zzp.id=$P{zahtjev}
ORDER BY ts.ciklus, pk.semestar, pp.naziv]]>
		</queryString>
		<field name="sifra" class="java.lang.String"/>
		<field name="naziv_predmeta" class="java.lang.String"/>
		<field name="ects" class="java.lang.Float"/>
		<field name="ocjena" class="java.lang.Integer"/>
		<field name="datum_u_indeksu" class="java.sql.Date"/>
		<field name="semestar_ocjene" class="java.lang.Integer"/>
		<field name="ciklus_ocjene" class="java.lang.Integer"/>
		<variable name="ectsImenaOcjena" class="java.util.List">
			<variableExpression><![CDATA[java.util.Arrays.asList("", "", "", "", "", "F", "E", "D", "C", "B", "A","IO","UO")]]></variableExpression>
			<initialValueExpression><![CDATA[new java.util.ArrayList()]]></initialValueExpression>
		</variable>
		<variable name="bosanskiImenaOcjena" class="java.util.List">
			<variableExpression><![CDATA[java.util.Arrays.asList("", "", "", "", "", "5 (pet)", "6 (šest)", "7 (sedam)", "8 (osam)", "9 (devet)", "10 (deset)", "ispunio obaveze", "uspješno odbranio")]]></variableExpression>
			<initialValueExpression><![CDATA[new java.util.ArrayList()]]></initialValueExpression>
		</variable>
	</subDataset>
	<subDataset name="Ukupne statistike" uuid="4c5fd6d4-cc06-4507-9ebd-113d30cf18b0">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="ZamgerDataAdapter.xml"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="zahtjev" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT COUNT(ko.predmet) broj_predmeta, 
ROUND(AVG(ko.ocjena), 2) prosjecna_ocjena, 
SUM(pp.ects) ukupno_ects
FROM konacna_ocjena as ko, ponudakursa as pk, 
	pasos_predmeta as pp, student_predmet as sp, studij as s, 
	tipstudija as ts, zahtjev_za_potvrdu zzp
WHERE ko.student=zzp.student and ko.pasos_predmeta=pp.id 
	and ko.predmet=pk.predmet 
	and ko.akademska_godina=pk.akademska_godina 
	and pk.id=sp.predmet and sp.student=zzp.student 
	and pk.studij=s.id and s.tipstudija=ts.id 
	and ko.ocjena>5 and zzp.id=$P{zahtjev}
ORDER BY ts.ciklus, pk.semestar, pp.naziv]]>
		</queryString>
		<field name="broj_predmeta" class="java.lang.Integer"/>
		<field name="prosjecna_ocjena" class="java.lang.Double"/>
		<field name="ukupno_ects" class="java.lang.Double"/>
	</subDataset>
	<parameter name="token" class="java.lang.Integer">
		<parameterDescription><![CDATA[Report token]]></parameterDescription>
	</parameter>
	<parameter name="zahtjev" class="java.lang.Integer">
		<parameterDescription><![CDATA[Zahtjev]]></parameterDescription>
	</parameter>
	<parameter name="zahtjev_datum" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT o.ime, o.imeoca, o.imemajke, o.spol,
o.prezime, o.brindexa, o.datum_rodjenja, o.jmbg,
m.naziv mjesto_rodjenja, op.naziv opcina_rodjenja,
d.naziv drzava_rodjenja, s.naziv naziv_studija, ss.semestar,
ss.ponovac, ss.status_studenta, ss.put, ag.naziv naziv_akademske_godine, 
ag2.naziv akademska_godina_studij, ko.ocjena polozen_zavrsni,
ko.datum_u_indeksu datum_diplomiranja,
ns.naziv nacin_studiranja_txt, sp.naziv svrha_potvrde,
ts.ciklus, tituliraj(i.dekan) dekan, zzp.datum_obrade,
CONCAT('00000' , zzp.id) zahtjev
FROM   osoba o, mjesto m, opcina op, drzava d,
       akademska_godina ag, akademska_godina ag2, 
       institucija i, svrha_potvrde sp, jasper_token jt,
       zahtjev_za_potvrdu zzp LEFT JOIN student_studij ss ON zzp.student=ss.student
     INNER JOIN (
         SELECT student, akademska_godina, MAX(semestar) msem
         FROM student_studij
         GROUP BY student, akademska_godina
     ) b ON ss.student=b.student and ss.akademska_godina=b.akademska_godina and ss.semestar=b.msem
        INNER JOIN (
         SELECT student, MAX(akademska_godina) mag
         FROM student_studij
         GROUP BY student
     ) c ON ss.student=c.student and ss.akademska_godina=c.mag
    LEFT JOIN studij s ON ss.studij=s.id
     LEFT JOIN tipstudija ts on s.tipstudija=ts.id
     LEFT JOIN nacin_studiranja ns on ss.nacin_studiranja=ns.id
     LEFT JOIN konacna_ocjena ko on ko.akademska_godina=ss.akademska_godina AND ko.student=ss.student AND
                                   ko.ocjena=12
 WHERE (zzp.id=$P{zahtjev} OR (zzp.datum_zahtjeva >= CONCAT($P{zahtjev_datum}, ' 00:00:00') AND zzp.datum_zahtjeva <= CONCAT($P{zahtjev_datum}, ' 23:59:59'))) AND zzp.student=o.id AND
zzp.akademska_godina=ag.id AND zzp.svrha_potvrde=sp.id AND
zzp.student=ss.student AND 
ss.akademska_godina = ag2.id AND
m.id=o.mjesto_rodjenja AND op.id=m.opcina AND
d.id=m.drzava AND i.tipinstitucije=1
AND zzp.tip_potvrde=2
AND jt.token=$P{token}
AND jt.report='Prepis' AND (jt.param1=$P{zahtjev} OR jt.param1=$P{zahtjev_datum})
AND NOW()-jt.vrijeme<1200]]>
	</queryString>
