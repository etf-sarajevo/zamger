	<parameter name="zahtjev_datum" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT o.ime, o.imeoca, o.imemajke, 
o.prezime, o.brindexa, o.datum_rodjenja, 
m.naziv mjesto_rodjenja, op.naziv opcina_rodjenja,
d.naziv drzava_rodjenja, s.naziv naziv_studija, ss.semestar,
ss.ponovac, ss.status_studenta, ss.put, ag.naziv naziv_akademske_godine, 
ns.naziv nacin_studiranja_txt, sp.naziv svrha_potvrde,
ts.ciklus, tituliraj(i.dekan) dekan, zzp.datum_obrade
FROM zahtjev_za_potvrdu zzp, osoba o, mjesto m, opcina op,
drzava d, studij s, akademska_godina ag, 
nacin_studiranja ns, svrha_potvrde sp, tipstudija ts,
institucija i, jasper_token jt, student_studij ss
INNER JOIN (
         SELECT student, akademska_godina, MAX(semestar) msem
         FROM student_studij
         GROUP BY student, akademska_godina
     ) b ON ss.student=b.student and ss.akademska_godina=b.akademska_godina and ss.semestar=b.msem
WHERE (zzp.id=$P{zahtjev} OR (zzp.datum_zahtjeva >= CONCAT($P{zahtjev_datum}, ' 00:00:00') AND zzp.datum_zahtjeva <= CONCAT($P{zahtjev_datum}, ' 23:59:59'))) AND zzp.student=o.id AND
zzp.akademska_godina=ag.id AND zzp.svrha_potvrde=sp.id AND
zzp.student=ss.student AND 
zzp.akademska_godina=ss.akademska_godina AND
ss.studij=s.id AND ss.nacin_studiranja=ns.id AND
m.id=o.mjesto_rodjenja AND op.id=m.opcina AND
d.id=m.drzava AND s.tipstudija=ts.id AND i.tipinstitucije=1
AND jt.token=$P{token}
AND jt.report='Potvrda' AND (jt.param1=$P{zahtjev} OR jt.param1=$P{zahtjev_datum})
AND NOW()-jt.vrijeme<1200
LIMIT 100]]>
	</queryString>
